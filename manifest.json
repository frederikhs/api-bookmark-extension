{
  "manifest_version": 3,
  "name": "Bookmark Syncer",
  "description": "sync bookmarks from remote json apis",
  "version": "0.1",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "host_permissions": ["https://pastebin.com/*", "https://gist.githubusercontent.com/*"],
  "icons": {
    "16": "icons/bookmark-16.png",
    "48": "icons/bookmark-48.png",
    "128": "icons/bookmark-128.png"
  },
  "action": {
    "default_title": "Bookmark Syncer",
    "default_icon": "icons/bookmark-128.png",
    "default_popup": "index.html"
  }
}
